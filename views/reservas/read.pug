extends ../layout

block content
  table.table
    thead
      tr
        th(scope="Listado") Listado
    thead
      tr
        th(scope="col") #
        th(scope="col") Color
        th(scope="col") Modelo
        th(scope="col") Ubicacion

    tbody
      each bici in bicis
        tr
          td= bici.code
          td= bici.color
          td= bici.modelo
          td= bici.ubicacion
